class sampleProgram {

	variables{
		int x,
		int y,
		int result
	}
	int exp(int base, int e)
	{
		variables{int i, int temp}
		temp = base;
		if(e > 0)
		{
			for(i = 1; i < e; i = i + 1)
			{
				base = base * temp;
			};
		}
		else
		{
			base = 1;
		};
		return base;
	}
	
	void main{
		x = 6
		y = 8
		result = exp(x, y);
		
	}	

}